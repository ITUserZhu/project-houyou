@import('vender.styl')
.user-head
	height 200px 
	background-image url('../../images/per-index-bg.png')
	background-repeat no-repeat
	padding 20px 55px 
	box-sizing border-box
	.fl 
		width 200px 
	.cen 
		width 100%
		.user-wra
			margin 0 200px 0 200px 
			text-align center
			.heading-img
				width 100px 
				height 100px 
				overflow hidden
				border-radius 50%
				margin 0 auto
				img 
					width 100%
					height 100%
			.nickname
				margin-top 12px
				font-size 18px
	.left
		margin-left -100%
		padding-top 22px
		p 
			line-height 32px 
			i 
				font-size 20px
				vertical-align text-bottom
				margin-right 5px
	.right 
		margin-left -200px
		.vip-type,.point-left
			display inline-block
			width 95px
			height 90px 
			text-align center
			padding-top 10px
			vertical-align middle
			position relative
			a 
				display inline-block
				width 60px 
				height 30px 
				line-height 30px 
				color #fff 
				text-align center
				border-radius 5px 
				background-color #222
				position absolute
				bottom 10px 
				left 50%
				margin-left -30px
		.point-left
			border-left 1px solid #fff
			p 
				font-size 16px
				font-weight bold
				margin-top -5px
		.sign
			width 60px 
			height 30px 
			line-height 30px 
			color #fff 
			text-align center
			border-radius 5px 
			background-color #333
			margin 10px auto 0
.menu
	font-size 0
	padding 5px 
	background-color rgba(0,0,0,.3)
	box-shadow inset 0 0 3px 1px hsla(0,0%,70%,.4)
	margin -25px auto 0
	width 828px
	border-radius 5px 
	a 
		display inline-block
		font-size 14px 
		width 100px 
		height 39px 
		line-height 40px 
		text-align center
		color #fff 
		margin-left 4px 
		background-color #333
		border-top 1px solid #5C5C5C
		&:first-child 
			margin-left 0
		&.active 
			background linear-gradient(to top, #FA1C31, #FF3346)
			border-radius 3px
.menu-item
	margin-top 24px
	min-height 350px
	position relative
	&>li 
		display none
		width 1160px
		margin 0 auto
		&.active 
			display block
		.item-type
			border-bottom 1px solid #999
			font-size 0
			span 
				font-size 14px
				display inline-block
				border 1px solid transparent
				margin 0 -1px -1px 0
				width 98px 
				height 38px 
				line-height 38px 
				text-align center
				cursor pointer
				&.active 
					border-color #999
		input[type=text],select, input[type=url]
			height 40px 
			font-size 16px 
			text-indent 10px 
			background-color #666
			outline none
			border none
			color #aaa
		input[type=text],input[type=url]
			width 770px
		:-moz-placeholder
			color #aaa
		::-moz-placeholder
			color #aaa 
		:-ms-input-placeholder
			color #aaa 
		::-webkit-input-placeholder
			color #aaa 
		.item-con
			.item 
				display none 
				&.active 
					display block
		// 个人信息
		&.per-info
			.item
				width 830px 
				height 385px 
				margin 66px auto 0 
				fieldset
					margin-bottom 20px
					&.sex,&.tel 
						display inline-block
					&.sex
						select
							width 180px
					&.tel 
						margin-left 120px
						input 
							width 300px 
						span 
							color #FA1C31
							text-decoration underline
							margin-left 20px
							cursor pointer
					.city-select
						display inline-block
						select 
							width 380px
							margin-right 10px
			.save-btn
				display block
				width 140px
				height 40px
				border-radius 5px 
				background-color #FA1C31
				text-align center
				line-height 40px 
				margin 35px auto 0
				outline none
				border none 
				color #fff
				cursor pointer
				&.grey 
					background-color #999
					cursor default
		// 收藏,下载
		&.collection,&.donload-his,&.history
			.item-con
				.item 
					&.soft,&.plugin
						li 
							width 210px 
							height 274px 
							float left
							margin 22px 27px 0 0
							position relative
							padding 25px 
							box-sizing border-box
							background-color rgba(0,0,0,.4)
							text-align center
							transform translateY(0)
							transition all .2s
							&:nth-child(5n)
								margin-right 0
							&:hover 
								transform translateY(-5px)
								span.del-col
									display block
								p.down
									background-color #FA1C31
							span 
								position absolute
								top 0px
								&.del-col
									top 8px
									right 10px
									width 16px
									height 16px 
									cursor pointer
									text-align center
									line-height 16px 
									border-radius 50%
									background-color #ddd 
									color #6b6b6b
									font-size 12px
									display none
								&.time
									left 0
									width 80px 
									height 20px 
									text-align center
									background-color rgba(131,131,131,.2)
									font-size 12px
							.img 
								width 95px 
								height 95px 
								overflow hidden
								margin 0 auto
								img 
									height 100%
									width 100%
							p 
								&.title 
									height 4.8em 
									overflow hidden
									margin-top 20px
								&.version 
									font-size 12px
								&.down 
									position absolute
									bottom 0px
									left 0px
									height 30px 
									line-height 30px 
									background-color #111
									width 100%	
					&.pic
						margin-top 20px
						li 
							float left
							margin 3px 
							overflow hidden
							position relative
							&:hover 
								.mask 
									transform translateY(0)
							img 
								width 100%
								height 100%
							.mask 
								position absolute
								width 100%
								height 100%
								left 0px
								top 0px
								background linear-gradient(to top, rgba(0,0,0,.8), rgba(0,0,0,0))
								padding 80px 0 0
								box-sizing border-box
								text-align center
								transform translateY(100%)
								transition all .4s
								span 
									position absolute
									right 10px
									top 10px
									width 60px 
									height 30px 
									line-height 30px
									background-color rgba(0,0,0,.3)
									border-radius 5px 
									text-align center
								p 
									position absolute
									width 100%
									left 0px
									bottom 0px
									line-height 30px 
									text-align left
									text-indent 10px

							
		
			
		// 充值记录,消费记录
		&.recharge,&.consume
			fieldset
				margin 20px 0
				select 
					width 120px
					margin-right 35px
				input 
					width 300px
				button 
					display inline-block
					width 140px
					vertical-align top
					height 40px
					background-color #FA1C31
					margin-left 20px
					text-align center
					line-height 40px 
					outline none
					border none 
					color #fff
					&.grey 
						background-color #999
					i 
						font-size 18px
						vertical-align text-bottom
						margin-right 5px
			table 
				width 100%
				tr 
					td,th
						text-align center
						word-break break-all
						padding 10px 0 10px 20px
					th 
						width 200px
					&:nth-child(2n)
						background-color rgba(0,0,0,.3)
				thead 
					tr 
						height 40px
						background-color #111
		// 优惠券
		&.coupon
			ul 
				width 1100px 
				margin 20px auto
			li 
				width 340px 
				height 100px 
				float left
				margin 0 40px 30px 0
				position relative
				background-color #C4A46F
				overflow hidden
				&:after,&:before 
					content ""
					position absolute
					left -10px
					width 20px 
					height 20px 
					background-color #232323
					border-radius 50%
					top -10px
				&:after
					top auto 
					bottom -10px
				&:nth-child(3n)
					margin-right 0
				&.active 
					.num
						.sign.unuse
							display block
					background-color #B0B0B0
				.fl 
					width 240px 
					height 100px 
					padding 10px 17px 
					box-sizing border-box
					position relative
					&:after,&:before 
						content ""
						position absolute
						right -12px
						width 20px 
						height 20px 
						background-color #232323
						border-radius 50%
						top -10px
						z-index 2
					&:after
						top auto 
						bottom -10px
					.con 
						width 100%
						height 100%
						border 1px solid #A58963
						padding 15px 30px 
						box-sizing border-box
						text-align center
						p 
							font-size 17px 
							&.time 
								font-size 12px 
								margin-top 8px 
				.num
					overflow hidden
					position relative
					width 96px 
					height 100%
					border-left 4px dotted #222 
					margin-left -4px
					text-align center
					line-height 100px 
					font-size 18px
					overflow hidden
					em 
						font-size 26px
					.sign
						position absolute
						font-size 14px 
						line-height 1.6
						right -32px
						top 9px
						transform rotate(45deg)
						width 100px 
						height 20px 
						line-height 20px 
						font-size 12px
						display none
						&.use 
							background-color #DB484E
							display block
						&.unuse 
							background-color #4E4E4E
		// 消息中心
		&.message
			.item-con
				padding-top 20px
			table 
				width 100%
				tr 
					td,th
						word-break break-all
						padding 10px 0 10px 20px
					th 
						width 200px
					&:nth-child(2n)
						background-color rgba(0,0,0,.3)
				thead 
					tr 
						height 40px
						background-color #111
			.msg-in
				table
					tr 
						td 
							i 
								display inline-block
								width 8px 
								height 8px 
								border-radius 50%
								background-color #999
								margin 0 15px 0 0
						td:nth-child(2n)
							text-align right
							padding-right 15px
			.report-list
				text-align center
			.report
				fieldset
					margin-bottom 20px
					.type 
						margin-right 20px
					label 
						position relative 
						margin-right 25px
						&.active 
							i:after 
								display block
						i 
							position absolute
							width 14px
							height 14px 
							background-color #131313
							border-radius 50%
							left -1px
							top 1px
							&:after
								content "" 
								position absolute
								width 6px 
								height 6px 
								left 4px 
								top 4px 
								background-color #FA1C31
								border-radius 50%
								display none
						input
							margin-right 8px
							outline none
					&.quest-cont
						margin-bottom 0
						.type 
							float left
							margin-right 24px
						div 
							overflow hidden
							textarea
								width 100%
								resize none
								background-color #666
								border none
								outline none
								padding 10px
								height 200px 
								color #aaa
								font-size 16px
				input[type='submit']
					display block
					width 140px
					height 40px
					border-radius 5px 
					background-color #FA1C31
					text-align center
					line-height 40px 
					margin-top 5px
					outline none
					border none 
					color #fff
					float right
				.alert-text
					margin-left 90px
					margin-top 6px 
					color #fa1c31
					display none
