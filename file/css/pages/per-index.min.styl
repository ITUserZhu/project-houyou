-info span{position:absolute;right:10px;top:7px;width:58px;height:28px;border:1px solid #FE812D;color:#FE812D;text-align:center;line-height:28px;font-size:16px}.pay-scan .ali-logo-r{height:50px;line-height:50px;border-radius:5px;background-color:#fff;margin:65px auto 0;width:90%;border:1px solid #FE6601}.renew-con .pay-scan .ali-logo-r img{vertical-align:middle;margin:0 25px}.renew-con .pay-scan .ali-logo-r .intro{color:#999}.renew-con .pay-scan .ali-logo-r .pay-num{float:right;font-size:16px;margin-right:20px}.renew-con .pay-scan .ali-logo-r .pay-num em{color:#FE6608}.renew-con .pay-scan form{line-height:1.5em}.renew-con .pay-scan .btn-ali{width:148px;height:38px;line-height:38px;color:#fff;letter-spacing:2px;font-size:16px;background-color:#fe6600;box-shadow:0 2px 0 0 #cc2a1e;position:relative;text-align:center;cursor:pointer;display:block;margin:40px auto;border-radius:5px;outline:0;border:none}

.tel-athu
	width 440px 
	height 340px 
	position fixed 
	left 0
	top 0
	bottom 0
	right 0
	margin auto 
	background-image:url('../../images/body-back.png')
	padding 40px 30px
	border-radius 5px 
	box-sizing border-box
	box-shadow 0 0 10px 0 rgba(0,0,0,.5);
	display none
	input[type=text]
		height 40px 
		font-size 16px 
		text-indent 10px 
		background-color #666
		outline none
		border none
		color #aaa
	:-moz-placeholder
		color #aaa
	::-moz-placeholder
		color #aaa 
	:-ms-input-placeholder
		color #aaa 
	::-webkit-input-placeholder
		color #aaa 
.tel-athu .toggle_view{display:none}.tel-athu .toggle_view.active{display:block}.tel-athu fieldset{margin-bottom:15px}.tel-athu fieldset.new-tel .tel-ipt,.tel-athu fieldset:first-child .tel-ipt{width:310px}.tel-athu fieldset:nth-child(2) img{width:100px;height:38px;border:1px solid #eee;vertical-align:top;margin-left:17px;cursor:pointer}.tel-athu fieldset:nth-child(3) label{letter-spacing:4px}.tel-athu fieldset.new-tel label{letter-spacing:0}.tel-athu fieldset .get-code{font-size:12px;width:98px;display:inline-block;height:38px;margin-left:17px;background-color:#F9FFFA;color:#fa1c31;border:1px solid #fa1c31;vertical-align:top;text-align:center;line-height:40px;border-radius:5px;cursor:pointer}.tel-athu fieldset .get-code.forbid{color:#999;border-color:#999;cursor:default}.tel-athu fieldset:nth-child(3){margin-bottom:30px}.tel-athu .submit_tel{display:block;margin:0 auto;background-color:#fa1c31;width:150px;height:40px;color:#fff;outline:0;border:none;border-radius:5px;margin-top:10px;cursor:pointer}.tel-athu .tel_tips,.tel-athu .tel_tips_past{text-align:center;margin-top:10px;color:#FF5A5A;font-size:12px;opacity:0}.tel-athu fieldset.change-code{position:relative}.tel-athu .change-tips{color:#999;margin:10px 0}.tel-athu .tel_tips_past.gactive{opacity:1;color:green}.tel-athu .tel_tips_past.ractive{opacity:1;color:#FF5A5A}.tel-athu .tel_tips.active{opacity:1}.tel-athu .get-code-tip{font-size:12px;color:#999;position:absolute;right:0;top:44px}

.tel-athu .close-tel {
	position: absolute;
    font-size: 30px;
    padding: 5px;
    text-align: center;
    top: -15px;
    right: -15px;
    cursor: pointer;
    color: #aaa;
    -webkit-border-radius: 50%;
    border-radius: 50%;
}
.tel-athu fieldset:first-child .per-tel {
	width 296px; 
}
.tel-athu .per-tel-code {
	width 179px;
}
.tel-athu fieldset.new-tel {
	margin-bottom 5px
}
.tel-athu fieldset.new-tel .tel-ipt {
	width 296px;
}
.tel-athu .new-tel {
	margin-top 25px
}
.no-data-show {
	display none 
	text-align center
	line-height 60px
	position absolute
	left 50%
	margin-left -26px
	top 100px
}
.no-data-show-tb {
	text-align center
	line-height 60px
}